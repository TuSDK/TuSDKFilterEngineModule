<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="144dp"
    android:background="@color/lsq_color_black">

    <ImageView
        android:id="@+id/lsq_back"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:background="@color/lsq_color_black"
        android:src="@drawable/backup_ic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageView
        android:scaleType="fitCenter"
        android:id="@+id/lsq_monster_remove"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="24dp"
        android:background="@color/lsq_alpha_background"
        android:src="@drawable/skin_no_ic"
        app:layout_constraintBottom_toTopOf="@id/lsq_back"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/lsq_skin_not_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        android:text="无"
        android:textColor="@color/text13"
        android:textSize="10sp"
        app:layout_constraintLeft_toLeftOf="@id/lsq_monster_remove"
        app:layout_constraintRight_toRightOf="@id/lsq_monster_remove"
        app:layout_constraintTop_toBottomOf="@id/lsq_monster_remove" />

    <ImageView
        android:scaleType="fitCenter"
        android:id="@+id/lsq_skin_mode_icon"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginStart="2dp"
        android:src="@drawable/skin_precision_ic"
        app:layout_constraintBottom_toBottomOf="@id/lsq_monster_remove"
        app:layout_constraintLeft_toRightOf="@id/lsq_monster_remove"
        app:layout_constraintTop_toTopOf="@id/lsq_monster_remove" />

    <TextView
        android:id="@+id/lsq_skin_mode_title"
        style="@style/lsq_module_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="精准美颜"
        app:layout_constraintBottom_toBottomOf="@id/lsq_skin_not_title"
        app:layout_constraintLeft_toLeftOf="@id/lsq_skin_mode_icon"
        app:layout_constraintRight_toRightOf="@id/lsq_skin_mode_icon"
        app:layout_constraintTop_toTopOf="@id/lsq_skin_not_title" />

    <View
        android:id="@+id/lsq_skin_background_point"
        android:layout_width="4dp"
        android:layout_height="4dp"
        android:layout_marginStart="12dp"
        android:background="@drawable/point"
        app:layout_constraintBottom_toBottomOf="@id/lsq_monster_remove"
        app:layout_constraintLeft_toRightOf="@id/lsq_skin_mode_icon"
        app:layout_constraintTop_toTopOf="@id/lsq_monster_remove" />

    <ImageView
        android:scaleType="fitCenter"
        android:layout_marginStart="20dp"
        android:id="@+id/lsq_skin_dermabrasion_icon"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:src="@drawable/skin_dermabrasion_ic"
        app:layout_constraintBottom_toBottomOf="@id/lsq_monster_remove"
        app:layout_constraintLeft_toRightOf="@id/lsq_skin_background_point"
        app:layout_constraintTop_toTopOf="@id/lsq_monster_remove"
         />

    <TextView
        android:id="@+id/lsq_skin_dermabrasion_title"
        style="@style/lsq_module_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="磨皮"
        app:layout_constraintBottom_toBottomOf="@id/lsq_skin_not_title"
        app:layout_constraintLeft_toLeftOf="@id/lsq_skin_dermabrasion_icon"
        app:layout_constraintRight_toRightOf="@id/lsq_skin_dermabrasion_icon"
        app:layout_constraintTop_toTopOf="@id/lsq_skin_not_title" />

    <ImageView
        android:scaleType="fitCenter"
        android:layout_marginLeft="16dp"
        android:id="@+id/lsq_skin_white_icon"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:src="@drawable/skin_white_ic"
        app:layout_constraintRight_toLeftOf="@id/lsq_skin_ruddy_icon"
        app:layout_constraintBottom_toBottomOf="@id/lsq_monster_remove"
        app:layout_constraintLeft_toRightOf="@id/lsq_skin_dermabrasion_icon"
        app:layout_constraintTop_toTopOf="@id/lsq_monster_remove"
        />

    <TextView
        android:id="@+id/lsq_skin_white_title"
        style="@style/lsq_module_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="美白"
        app:layout_constraintBottom_toBottomOf="@id/lsq_skin_not_title"
        app:layout_constraintLeft_toLeftOf="@id/lsq_skin_white_icon"
        app:layout_constraintRight_toRightOf="@id/lsq_skin_white_icon"
        app:layout_constraintTop_toTopOf="@id/lsq_skin_not_title" />



    <ImageView
        android:scaleType="fitCenter"
        android:id="@+id/lsq_skin_ruddy_icon"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="17dp"
        android:src="@drawable/skin_red_ic"
        app:layout_constraintBottom_toBottomOf="@id/lsq_monster_remove"
        app:layout_constraintLeft_toRightOf="@id/lsq_skin_white_icon"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@id/lsq_monster_remove" />

    <TextView
        android:id="@+id/lsq_skin_ruddy_title"
        style="@style/lsq_module_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="红润"
        app:layout_constraintBottom_toBottomOf="@id/lsq_skin_not_title"
        app:layout_constraintLeft_toLeftOf="@id/lsq_skin_ruddy_icon"
        app:layout_constraintRight_toRightOf="@id/lsq_skin_ruddy_icon"
        app:layout_constraintTop_toTopOf="@id/lsq_skin_not_title" />
</androidx.constraintlayout.widget.ConstraintLayout>